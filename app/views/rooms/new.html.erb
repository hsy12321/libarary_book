<% provide(:title, 'Create Room') %>
<h1>Create Room</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@room) do |f| %>
      <%= render 'shared/errorcr_messages' %>
    
      <%= f.label :rommid, "Room id" %>
      <%= f.text_field :rommid, class: 'form-control' %>

      <%= f.label :build %>
      <%= f.text_field :build, class: 'form-control' %>

      <%= f.label :size %>
      <%= f.text_field :size, class: 'form-control' %>


      <%= f.submit "Create a room", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>